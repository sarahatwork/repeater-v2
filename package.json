{
  "name": "repeater-v2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.17.1",
    "@contentful/f36-components": "4.37.0",
    "@contentful/f36-tokens": "4.0.1",
    "@contentful/field-editor-single-line": "^1.2.0",
    "@contentful/field-editor-test-utils": "^1.3.0",
    "@contentful/react-apps-toolkit": "1.2.15",
    "@emotion/react": "^11.10.6",
    "contentful-management": "10.32.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "tsc": "tsc",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "update-app-definition": "ts-node ./scripts/update-app-definition.ts",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.8.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@tsconfig/create-react-app": "1.0.3",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "cross-env": "7.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "homepage": ".",
  "type": "module"
